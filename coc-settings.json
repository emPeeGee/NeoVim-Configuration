{
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "less",
    "scss",
    "rust",
    "go"
  ],
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "cSpell.userWords": [
    "Gdiffsplit",
    "Moldovanu",
    "NLWS",
    "Oxocarbon",
    "Tpope",
    "aquib",
    "blinkon",
    "frecency",
    "gruvbox",
    "higroup",
    "hlslens",
    "lcov",
    "linehl",
    "mygroup",
    "neorg",
    "noice",
    "norg",
    "numhl",
    "structs",
    "testid",
    "testkit"
  ],
  "diagnostic.checkCurrentLine": true,
  "diagnostic.showUnused": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": " | ",
  "cSpell.showStatus": true,
  "cSpell.ignoreRegExpList": ["['\"/-]\\w+['\"/-]"],
  "cSpell.diagnosticLevel": "Hint",
  "jest.watch": false,
  "diagnostic.signPriority": 2,
  "diagnostic.enableSign": true,
  "diagnostic.infoSign": "!",
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell", "hs", "lhs"],
      // Settings are optional, here are some example values
      "settings": {
        "haskell": {
          "checkParents": "CheckOnSave",
          "checkProject": true,
          "maxCompletions": 40,
          "formattingProvider": "ormolu",
          "plugin": {
            "stan": { "globalOn": true }
          }
        }
      }
    }
  },
  "Lua.telemetry.enable": false
}
